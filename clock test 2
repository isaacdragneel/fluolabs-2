def setup():
    size(400, 400)
    
def draw():
    background(211)
    stroke(255)
    textSize(32)
    stroke(0)
    fill(0)
    rect(0, 300, 400, 400, 5)
    
    h = hour() - 12
    m = minute()
    s = second() 
    
    fill(255)
    text(str(h) + "h" + str(m) + "m" + str(s) + "s", 120, 300)
    
    stroke(0, 176, 255)
    fill(0, 176, 255)
    rect(200, 200, 50, 50, 5)
    
    stroke(0, 255, 0)
    fill(0, 255, 0)
    rect(320, 249, 50, 50, 5)
